{
    "manifestVersion": 1,
    "id": "azure-solution-scaffolding",
    "publisher": "SeanMa",
    "version": "1.0.5",
    "name": "Azure Solution Scaffolding",
    "description": "ADO Extension for Azure Solution Scaffolding",
    "public": true,
    "galleryFlags": [
        "Preview"
    ],
    "categories": ["Azure Repos"],
    "baseUri": "https://scaffolding-repos.github.io/ado-scaffolding/",
    "scopes": [
        "vso.build_execute",
        "vso.project_manage",
        "vso.work_full",
        "vso.release_manage",
        "vso.settings",
        "vso.variablegroups_manage",
        "vso.code_full",
        "vso.tokens",
        "vso.tokenadministration",
        "vso.security_manage",
        "vso.machinegroup_manage",
        "vso.environment_manage",
        "vso.taskgroups_manage",
        "vso.serviceendpoint_manage"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "my-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Scaffolding",
                "uri": "index.html"
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        }
        
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://github.com/scaffolding-repos/ado-scaffolding"
        },
        "getstarted": {
            "uri": "https://github.com/scaffolding-repos/ado-scaffolding/blob/dev/README.md"
        },
        "learn": {
            "uri": "https://github.com/scaffolding-repos/ado-scaffolding/blob/dev/README.md"
        },
        "support": {
            "uri": "https://github.com/scaffolding-repos/ado-scaffolding/issues"
        },
        "repository": {
            "uri": "https://github.com/scaffolding-repos/ado-scaffolding"
        },
        "issues": {
            "uri": "https://github.com/scaffolding-repos/ado-scaffolding/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/scaffolding-repos/ado-scaffolding"
    },
    "branding": {
        "color": "rgb(34, 34, 34)",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "screenshots/extension_ui.png"
        },
        {
            "path": "screenshots/scaffolding.png"
        },
        {
            "path": "screenshots/import_zip.png"
        },
        {
            "path": "screenshots/settings.png"
        },
        {
            "path": "screenshots/preview.png"
        }
    ],
    "icons": {
        "default": "public/icon-128-128.png"
    }
}